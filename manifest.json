{
  "name": "Video Speed Controller",
  "description": "Control the playback speed of videos on any website.",
  "manifest_version": 3,
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "icons": {
    "48": "icon48.png",
    "96": "icon96.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}